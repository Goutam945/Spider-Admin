<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp"
    android:background="@color/black"
    tools:context=".Website_list">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        app:cardElevation="8dp"
        app:cardMaxElevation="5dp"
        app:cardBackgroundColor="@color/d_black"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:background="@color/d_black"
            android:layout_marginBottom="10dp"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:orientation="vertical"
                android:padding="4dp">

                <TextView
                    android:id="@+id/tv_webname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Spider"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    android:fontFamily="@font/gtwalsheimpro_regular"/>
                <TextView
                    android:id="@+id/tv_url"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:text="spider.com"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:fontFamily="@font/gtwalsheimpro_regular"/>
                <TextView
                    android:id="@+id/tv_desciption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="description"
                    android:layout_marginTop="3dp"
                    android:textColor="@color/white"
                    android:textSize="12dp"
                    android:fontFamily="@font/gtwalsheimpro_regular"/>

                <!--<LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:gravity="right"
                    android:orientation="horizontal">
                    <de.hdodenhof.circleimageview.CircleImageView
