<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="MissingConstraints"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toStartOf="@+id/linearLayout3"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/t5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Name"
            android:textColor="@color/white" />

        <EditText
            android:id="@+id/dname"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Number"
            android:textColor="@color/white" />

        <EditText
            android:id="@+id/detailp"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Bank Name"
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/dbankname"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="AccountNo."
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/accountno"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="IFC Code"
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/ifc"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Branch"
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/branch"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/t7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Holder Name"
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/holdername"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/t8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Account type"
            android:textColor="@color/white"
            android:visibility="gone" />

        <EditText
            android:id="@+id/accouttype"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:backgroundTint="@color/black"
            android:fontFamily="@font/gtwalsheimpro_regular"
            android:text="Paynet"
            android:textColor="@color/white"
            android:visibility="gone"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btn_send"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/bg_edit_text1"
            android:backgroundTint="@color/apptheamcolor"
            android:text="Save"
            android:textColor="@color/White"
            android:textSize="17dp"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent">

        <ImageView
            android:id="@+id/img_edit"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_edit_24" />

        <ImageView
            android:id="@+id/img_delete"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_baseline_delete_24" />
    </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>




